<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:color="#633e3e" /> <!-- focused -->
    <item android:state_pressed="true" android:color="#03f791" /> <!-- pressed -->
    <item android:state_selected="true" android:color="#f21010" /> <!-- pressed -->
