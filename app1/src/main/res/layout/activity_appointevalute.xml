<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.example.administrator.myyushi.appointevaluteActivity">
    <RelativeLayout
        android:background="#fbd23a"
        android:id="@+id/pingjia_ry1"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"
            android:id="@+id/appointyuyue_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/yy_jt_left"/>
        <TextView
            android:textColor="#000"
            android:textSize="20dp"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/evaluate"/>
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/apponitYuyue_kefu"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ddkefu"/>
    </RelativeLayout>



    <View
        android:id="@+id/appointyuyue_view"
        android:layout_below="@+id/pingjia_ry1"
        android:background="#c2bbbb"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
  <ListView
      android:background="#fff"
      android:id="@+id/appoint_lv"
      android:layout_below="@+id/appointyuyue_view"
      android:layout_width="match_parent"
      android:layout_height="match_parent">
  </ListView>
</RelativeLayout>
